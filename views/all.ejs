<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <style>
      .box {
        padding: 1rem;
        border: 1px solid black;
      }
    </style>
    <title>All</title>
  </head>
  <body>
    <% for(let user of users) { %>
    <div class="box">
      <h2><%= user.name %></h2>
      <p><%= user.email %></p>
      <p><%= user.phone %></p>
    </div>
    <% } %>
  </body>
</html>
